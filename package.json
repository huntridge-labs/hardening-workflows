{
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@j-ulrich/release-it-regex-bumper": "^5.3.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.4",
    "is-ci": "^4.1.0",
    "pre-commit": "^1.2.2",
    "release-it": "^19.0.5"
  },
  "dependencies": {
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.1",
    "@octokit/plugin-paginate-rest": "^13.2.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cz-emoji-conventional": "^1.0.2",
    "dateformat": "^5.0.3",
    "husky": "^9.1.4"
  },
  "scripts": {
    "prepare": "husky",
    "test": "npm ci",
    "commitizen": "commitizen init cz-conventional-changelog --save-dev --save-exact --force",
    "release": "release-it",
    "release:dry": "release-it --dry-run",
    "release:ci": "release-it --ci",
    "release:preview": "release-it --dry-run --ci",
    "release:changelog": "release-it --changelog --dry-run"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "version": "2.3.1"
}
